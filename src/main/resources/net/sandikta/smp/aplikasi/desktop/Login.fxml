<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.sandikta.smp.aplikasi.desktop.LoginController">
   <children>
      <Button fx:id="btnLogin" layoutX="122.0" layoutY="203.0" mnemonicParsing="false" onAction="#login" text="Login">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TextField fx:id="txUserName" layoutX="68.0" layoutY="79.0" promptText="UserName">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txPassword" layoutX="68.0" layoutY="149.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblStatus" alignment="CENTER" contentDisplay="CENTER" layoutX="23.0" layoutY="31.0" prefHeight="15.0" prefWidth="259.0" text="Welcome">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
