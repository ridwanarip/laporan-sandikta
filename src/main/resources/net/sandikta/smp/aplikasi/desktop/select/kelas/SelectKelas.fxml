<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="net.sandikta.smp.aplikasi.desktop.select.kelas.SelectKelasController">
   <children>
      <Label fx:id="lblSelectKelas" alignment="CENTER" layoutX="26.0" layoutY="35.0" prefHeight="17.0" prefWidth="357.0" text="Select Kelas Siswa">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <HBox layoutX="65.0" layoutY="93.0" prefHeight="87.0" prefWidth="280.0">
         <children>
            <VBox prefHeight="87.0" prefWidth="100.0">
               <children>
                  <Label fx:id="lblPilihKelas" prefHeight="25.0" prefWidth="100.0" text="Kelas">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="lblPilihSemester" layoutX="10.0" layoutY="10.0" prefHeight="25.0" prefWidth="100.0" text="Semester">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="15.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
            <VBox>
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
               <children>
                  <ComboBox fx:id="comboKelas" onAction="#comboPilihKelas" prefWidth="150.0" promptText="Pilih Kelas" />
                  <ComboBox fx:id="comboSemester" layoutX="10.0" layoutY="10.0" onAction="#comboPilihSemester" prefWidth="150.0" promptText="Pilih Semester">
                     <VBox.margin>
                        <Insets top="15.0" />
                     </VBox.margin>
                  </ComboBox>
               </children>
            </VBox>
         </children>
      </HBox>
      <Button fx:id="btnPilih" layoutX="200.0" layoutY="210.0" mnemonicParsing="false" onAction="#pilihKelas" prefHeight="25.0" prefWidth="86.0" text="Pilih" />
   </children>
</AnchorPane>
